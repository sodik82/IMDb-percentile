{
  "name": "IMDb percentile",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "This extension shows percentile for IMDb ratings",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.imdb.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.imdb.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}